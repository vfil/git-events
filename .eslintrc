{
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "prefer-const": 0,
    "no-unused-vars": 0,
    "comma-dangle": 0,
    "id-length": 0,
    "vars-on-top": 0,
    "no-var": 0,
    "no-nested-ternary": 0,
    "indent": 0,
    "semi": 0,
    "react/prop-types": 0,
    "spaced-comment": 0,
    "eol-last": 0,
    "react/sort-comp": 0,
    "curly": 0,
    "pace-after-keywords": 0,
    "space-after-keywords": 0,
    "one-var": 0,
    "semi-spacing": 0,
    "yoda": 0,
    "padded-blocks": 0,
    "react/jsx-boolean-value": 0,
    "space-infix-ops": 0,
    "space-before-function-paren": 0,
    "no-cond-assign": 0,
  }
}